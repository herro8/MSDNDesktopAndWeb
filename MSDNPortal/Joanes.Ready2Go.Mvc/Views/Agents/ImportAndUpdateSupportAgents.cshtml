@inherits System.Web.Mvc.WebViewPage

@{
    ViewBag.Title = "ImportAndUpdateSupportAgents";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="wrapper">

    <div class="sidebar">


        <div class="nContainer">


            @RenderPage("~/Views/Shared/_Menu.cshtml")


            <a class="close">
                <span class="ico-remove"></span>
            </a>
        </div>


    </div>

    <div class="body">

        <div class="content">

            <div class="page-header">
                <div class="icon">
                    <span class="ico-arrow-right"></span>
                </div>
                <h1> Refresh Support Agents List <small>upload support agents and refresh them in database.</small></h1>
            </div>

            <div class="row-fluid">
                <div class="span6">
                    <div class="block">
                        <div class="head">
                            <h2>You could upload txt file.</h2>
                            <ul class="buttons">
                            </ul>
                        </div>


                     


                        <form action="~/Agents/ImportAndUpdateSupportAgents" method="post" enctype="multipart/form-data">
                            <div class="data-fluid">
                                <div class="row-form">
                                    <div class="span3">File:</div>
                                    <div class="span9">
                                        <div class="input-append file">
                                            <input type="file" name="file" />
                                            <input type="text" />
                                            <button class="btn">Browse</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="data-fluid">
                                <div class="row-form">
                                    <div class="span3"></div>
                                    <div class="span9">
                                        <button type="submit" class="btn" id="submitbutton">Upload and Refresh to DataBase Now!</button>
                                    </div>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>


            
            </div>

         






        </div>

    </div>

</div>




